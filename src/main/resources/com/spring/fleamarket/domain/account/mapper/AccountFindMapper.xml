<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.fleamarket.domain.account.mapper.AccountFindMapper">

	<select id="selectAccountByName" resultType="Account">
		SELECT * FROM accounts WHERE name = #{name}
	</select>
	
	<select id="selectAccountImageByAccountId" resultType="string">
		SELECT path FROM account_images WHERE account_id = #{accountId}
	</select>
	
</mapper>